    "keywords": ["laravel","illuminate","auth","roles","acl","permission"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zizaco Zizuini",
            "email": "zizaco@gmail.com"
        },
        {
            "name": "Andrew Elkins",
            "homepage": "http://andrewelkins.com"
        },
        {
            "name": "Ben Batschelet",
            "homepage": "http://github.com/bbatsche"
        },
        {
            "name": "Michele Angioni",
            "email": "michele.angioni@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "4.0.x",
        "laravelbook/ardent": "*",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "illuminate/database": "4.0.x",
        "mockery/mockery": "0.7.2",
        "zizaco/factory-muff": "dev-master"
